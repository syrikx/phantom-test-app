{
  "expo": {
    "name": "phantom-test-app",
    "slug": "phantom-test-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "phantomtestapp",
    "bundleIdentifier": "com.phantomtestapp.app",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "297652e4-6810-4cc1-b918-700358aaee05"
      }
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "phantom-test-app",
            "CFBundleURLSchemes": [
              "phantomtestapp"
            ]
          },
          {
            "CFBundleURLName": "phantom-test-app",
            "CFBundleURLSchemes": [
              "phantomtestapp"
            ]
          }
        ]
      },
      "bundleIdentifier": "com.syrikx.phantomtestapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "phantomtestapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "phantomtestapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.syrikx.phantomtestapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      "expo-router"
    ]
  }
}
